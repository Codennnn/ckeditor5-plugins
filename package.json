{
  "name": "ckeditor5-plugins",
  "version": "0.1.0",
  "description": "CKEditor 5 插件集合。",
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "files": [
    "lang",
    "src/**/*.js",
    "src/**/*.d.ts",
    "theme",
    "build",
    "ckeditor5-metadata.json"
  ],
  "scripts": {
    "dev": "ckeditor5-package-tools start",
    "lint": "run-p lint:ts lint:es",
    "build": "run-p dll:build ts:build",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "lint:es": "eslint **/*.{js,ts}",
    "lint:css": "stylelint theme/**/*.css",
    "lint:prettier": "prettier --write **/*.{md,json}",
    "prepare": "yarn run dll:build",
    "prepublishOnly": "yarn run ts:build && ckeditor5-package-tools export-package-as-javascript",
    "postpublish": "yarn run ts:clear && ckeditor5-package-tools export-package-as-typescript",
    "test": "ckeditor5-package-tools test",
    "translations:collect": "ckeditor5-package-tools translations:collect",
    "translations:download": "ckeditor5-package-tools translations:download",
    "translations:upload": "ckeditor5-package-tools translations:upload",
    "dll:build": "ckeditor5-package-tools dll:build",
    "dll:serve": "http-server ./ -o sample/dll.html",
    "ts:build": "tsc -p ./tsconfig.release.json",
    "ts:clear": "npx rimraf src/**/*.{js,d.ts}"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-autoformat": "39.0.1",
    "@ckeditor/ckeditor5-basic-styles": "39.0.1",
    "@ckeditor/ckeditor5-block-quote": "39.0.1",
    "@ckeditor/ckeditor5-code-block": "39.0.1",
    "@ckeditor/ckeditor5-core": "39.0.1",
    "@ckeditor/ckeditor5-editor-classic": "39.0.1",
    "@ckeditor/ckeditor5-essentials": "39.0.1",
    "@ckeditor/ckeditor5-font": "39.0.1",
    "@ckeditor/ckeditor5-heading": "39.0.1",
    "@ckeditor/ckeditor5-image": "39.0.1",
    "@ckeditor/ckeditor5-indent": "39.0.1",
    "@ckeditor/ckeditor5-inspector": "4.1.0",
    "@ckeditor/ckeditor5-link": "39.0.1",
    "@ckeditor/ckeditor5-list": "39.0.1",
    "@ckeditor/ckeditor5-media-embed": "39.0.1",
    "@ckeditor/ckeditor5-package-tools": "^1.0.0",
    "@ckeditor/ckeditor5-paragraph": "39.0.1",
    "@ckeditor/ckeditor5-table": "39.0.1",
    "@ckeditor/ckeditor5-theme-lark": "39.0.1",
    "@ckeditor/ckeditor5-upload": "39.0.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "ckeditor5": "39.0.1",
    "eslint-config-ckeditor5": "^4.4.0",
    "http-server": "^14.1.0",
    "nanoid": "4.0.2",
    "npm-run-all": "^4.1.5",
    "prefer-code-style": "1.5.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "ckeditor5": "39.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=5.7.1"
  }
}
